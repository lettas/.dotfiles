[user]
  name  = lettas
  email = lettas0726@gmail.com

[color]
  ui = auto

[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | [%an] %s%d\" --graph --date=short
  graph = log --graph --date-order -C -M --pretty=format:\"<%h> %ad [%an] %Cgreen%d%Creset %s\" --all --date=short
  amend = commit --amend -C HEAD
  changed = whatchanged --pretty=format:'%Cgreen%h%Creset %ad [%Cblue%an%Creset] %s%Cred%d%Creset'
